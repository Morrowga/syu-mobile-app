{
  "expo": {
    "name": "sticker",
    "slug": "sticker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"  
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.morph.syu",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.morph.syu",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/BonaNovaSC-Regular.ttf",
            "./assets/fonts/BonaNovaSC-Bold.ttf",
            "./assets/fonts/Overpass-Regular.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "60ddfe6a-f8ec-45fe-8972-5d8450acc283"
      }
    }
  }
}
